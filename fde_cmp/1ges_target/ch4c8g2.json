    "code": {
        "BFmate": [
            "COMMON double ^inelmc8[nelem][m];",
            "Insr_C: double pe,pv,fx,fy,fz,rou,alpha,yita,csp,str[7];",
            "Insr_C: double ^d[3][3],^rq[3][3],snv[4],ssv[4];",
            "Insr_C: static int init=0;",
            "Insr_C: int m;",
            "$I",
            "Insr_C: m = ngaus*6;",
            "Insr_C: if (init==0) {",
            "Insr_C: inelmc8 = (double *) calloc(nelem*m,sizeof(double));",
            "Insr_C: for (i=0; i<nelem*m; ++i) inelmc8[i]=0.0;",
            "Insr_C: init = 1; }",
            "Oprt_Asgn: singular.xyz"
        ],
        "AFmate": [
            "Tnsr_Asgn: ^rq_i_j = revq_i_j/yita"
        ],
        "func": [
            "Oprt_Asgn: vol.xyz",
            "Insr_C: fact = pe/(1.+pv)/(1.-2.*pv);",
            "Insr_C: shear = (0.5-pv)*fact;",
            "Insr_C: j = (igaus-1)*6;",
            "Insr_C: for (i=1; i<=6; ++i) str[i]=^inelmc8[num][j+i];",
            "Tnsr_Asgn: ^d_i_j = sm_i_j*fact",
            "Tnsr_Asgn: snv_i = +^rq_i_j*sv_j",
            "Tnsr_Asgn: ssv_i = +^d_i_j*snv_j",
            "Tnsr_Asgn: ssv_i = ssv_i*dt - sv_i",
            "Tnsr_Asgn: csp = shear/yita*dt - 1.0",
            "Oprt_Asgn: [fe_i_j]=gradv.xyz()",
            "Func_Asgn: ev_i=fe[1,5,9]",
            "Func_Asgn: [fe_i_j]=+[fe_i_j]+[fe_j_i]",
            "Func_Asgn: ep_i=fe[6,3,2]"
        ]
    },
